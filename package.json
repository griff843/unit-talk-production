{
  "name": "unit-talk-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade betting automation platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "worker": "tsx src/worker.ts",
    "worker:start": "tsx src/worker.ts",
    "worker:dev": "tsx --watch src/worker.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "metrics:show": "node scripts/showMetrics.js",
    "health:check": "node scripts/healthCheck.js",
    "agents:test": "tsx src/runner/testAllAgents.ts",
    "agents:recap": "tsx src/runner/recapAgentTestHarness.ts",
    "agents:feed": "tsx src/runner/feedAgentTestHarness.ts",
    "clean": "rimraf dist",
    "clean:install": "rimraf node_modules package-lock.json && npm install",
    "docker:build": "docker build -t unit-talk-platform .",
    "docker:run": "docker run -p 3000:3000 unit-talk-platform",
    "prepare": "husky install"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@supabase/supabase-js": "^2.50.0",
    "@temporalio/activity": "^1.8.0",
    "@temporalio/client": "^1.8.0",
    "@temporalio/worker": "^1.8.0",
    "@temporalio/workflow": "^1.8.0",
    "axios": "^1.9.0",
    "date-fns": "^3.6.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.5.0",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "luxon": "^3.6.1",
    "node-cron": "^3.0.3",
    "openai": "^4.103.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prom-client": "^15.1.3",
    "redis": "^4.7.0",
    "twilio": "^4.19.0",
    "uuid": "^10.0.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/luxon": "^3.6.2",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemailer": "^7.0.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.6.0",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "private": true,
  "license": "UNLICENSED",
  "keywords": [
    "betting",
    "automation",
    "sports",
    "typescript",
    "temporal",
    "supabase",
    "enterprise"
  ],
  "repository": {
    "type": "git",
    "url": "private"
  },
  "bugs": {
    "url": "private"
  },
  "homepage": "private"
}